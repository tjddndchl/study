package pratice.submit01;

import java.util.ArrayList;

public class Example7 {

	public static void main(String[] args) {
		ArrayList<Integer>weekLotto =null;
		weekLotto =makeLotto();
		System.out.println(weekLotto);
		String inputarr = {"15","12"};

	}
	
	public static ArrayList<Integer> makeLotto(){
		ArrayList<Integer> num = new ArrayList<Integer>();
		for(int i=0; i<6;i++) {
			num.add(i, (int)(Math.random()*45+1));
		}
		return num;
		
	}
	
	public static ArrayList<Integer> makeLotto(String[] arr){
		ArrayList<Integer> num = new ArrayList<Integer>();
		String[] inputarr= new String[6];
		for(int i=0; i<6;i++) {
			num.add(i, (int)(Math.random()*45+1));
		}
		return num;
		return inputarr;
	
	}

}
